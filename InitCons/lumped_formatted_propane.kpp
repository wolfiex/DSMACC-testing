    PA(3)=0&
      +C(ind_LMP1)*LC(2)*C(ind_HO2)*KAPHO2*0.56
    LC(3) =  PA(3)/(PA(1)+PA(2)+PA(3))
    
! LMP2: 4 -> C3PAN1, 5 -> HOC2H4CO3, 6 -> HOC2H4CO3H

    PA(4)=0&
      +C(ind_LMP2)*LC(5)*C(ind_NO2)*KFPAN
    LC(4) =  PA(4)/(PA(4)+PA(5)+PA(6))
    PA(5)=0&
      +C(ind_HOC2H4CHO)*C(ind_NO3)*KNO3AL*2.4&
      +C(ind_HOC2H4CHO)*C(ind_OH)*3.06D-11&
      +C(ind_LMP2)*LC(6)*C(ind_OH)*1.73D-11&
      +C(ind_LMP2)*LC(4)*KBPAN
    LC(5) =  PA(5)/(PA(4)+PA(5)+PA(6))
    PA(6)=0&
      +C(ind_HO2)*C(ind_LMP2)*LC(5)*KAPHO2*0.41
    LC(6) =  PA(6)/(PA(4)+PA(5)+PA(6))

do index = 1, 6
  if (isnan(LC(index))) LC(index) = 0
end do


