! LMP1: 1 -> IPROPOLPAN, 2 -> CH3CHOHCO3, 3 -> IPROPOLPER

    PA(1)=0&
      +C(ind_LMP1)*LC(2)*C(ind_NO2)*KFPAN
    LC(1) =  PA(1)/(PA(1)+PA(2)+PA(3))
    PA(2)=0&
      +C(ind_CH3CHOHCHO)*C(ind_NO3)*KNO3AL*2.4&
      +C(ind_CH3CHOHCHO)*C(ind_OH)*1.7D-11&
      +C(ind_LMP1)*LC(3)*C(ind_OH)*9.34D-12&
      +C(ind_LMP1)*LC(1)*KBPAN
